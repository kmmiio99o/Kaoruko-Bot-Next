{
  "name": "kaoruko-bot-next",
  "version": "1.0.0",
  "description": "Kaoruko is a Discord bot with many commands",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "bun run scripts/build.js",
    "start": "bun run dist/index.js",
    "dev": "bun run scripts/dev.js",
    "watch": "bun --watch src/index.ts",
    "clean": "rm -rf dist node_modules",
    "lint": "bunx @biomejs/biome check .",
    "lint:fix": "bunx @biomejs/biome check --apply .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "dependencies": {
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ],
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "bun"
  ],
  "author": "Kaoruko Bot Team",
  "license": "MIT"
}
