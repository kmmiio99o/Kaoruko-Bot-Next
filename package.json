{
  "name": "kaoruko-bot-next",
  "version": "1.0.0",
  "description": "Kaoruko is a Discord bot with many commands",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node dist/index.js",
    "dev": "node scripts/dev.js",
    "watch": "npm exec tsc --watch --preserveWatchOutput",
    "clean": "rm -rf dist node_modules",
    "lint": "npm exec @biomejs/biome check .",
    "lint:fix": "npm exec @biomejs/biome check --apply .",
    "format": "npm exec @biomejs/biome format --write ."
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^20.14.12",
    "nodemon": "^3.1.4",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "npm"
  ],
  "author": "Kaoruko Bot Team",
  "license": "MIT"
}
